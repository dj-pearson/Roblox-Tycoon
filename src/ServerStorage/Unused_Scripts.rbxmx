				if tycoon and tycoon.Value then
					if descendant:IsDescendantOf(tycoon.Value) then
						RevenueDisplay.createRevenueDisplay(descendant, player)
						break
					end
				end
			end
		end
	end)

	debugPrint("Revenue Display System initialized")
	return true
end

